N			LPC_PWM1->PCR |= PWMENA1;		
X			((LPC_PWM_TypeDef *) ((0x40000000UL) + 0x18000) )->PCR |= (1 << 9);		
N		}
N		if(channel & (1<<1))
N		{	
N			/*Configura P2.1 para PWM1_2*/
N			LPC_PINCON->PINSEL4 &= ~(1<<3); 
X			((LPC_PINCON_TypeDef *) ((0x40000000UL) + 0x2C000) )->PINSEL4 &= ~(1<<3); 
N			LPC_PINCON->PINSEL4 |=  (1<<2);
X			((LPC_PINCON_TypeDef *) ((0x40000000UL) + 0x2C000) )->PINSEL4 |=  (1<<2);
N			LPC_PWM1->PCR |= PWMENA2;		
X			((LPC_PWM_TypeDef *) ((0x40000000UL) + 0x18000) )->PCR |= (1 << 10);		
N		}
N		if(channel & (1<<2))
N		{	
N			/*Configura P2.2 para PWM1_3*/
N			LPC_PINCON->PINSEL4 &= ~(1<<5); 
X			((LPC_PINCON_TypeDef *) ((0x40000000UL) + 0x2C000) )->PINSEL4 &= ~(1<<5); 
N			LPC_PINCON->PINSEL4 |=  (1<<4);
X			((LPC_PINCON_TypeDef *) ((0x40000000UL) + 0x2C000) )->PINSEL4 |=  (1<<4);
N			LPC_PWM1->PCR |= PWMENA3;		
X			((LPC_PWM_TypeDef *) ((0x40000000UL) + 0x18000) )->PCR |= (1 << 11);		
N		}
N	}
N
N	LPC_PWM1->PR  = 0x00;				/*Prescaler  count frequency:Fpclk*/ 
X	((LPC_PWM_TypeDef *) ((0x40000000UL) + 0x18000) )->PR  = 0x00;				  
N	LPC_PWM1->MCR = PWMMR0I;			/*Interrupt on PWMMR0, reset on PWMMR0, reset  TC if PWM matches*/ 
X	((LPC_PWM_TypeDef *) ((0x40000000UL) + 0x18000) )->MCR = (1 << 0);			  
N	PWM_Set(PWM_freq_KHz);				/*Configura a ultima frequencia configurada*/
N
N	/*counter enable, PWM enable*/
N	LPC_PWM1->TCR = TCR_CNT_EN | TCR_PWM_EN;	 
X	((LPC_PWM_TypeDef *) ((0x40000000UL) + 0x18000) )->TCR = 0x00000001 | 0x00000008;	 
N}
