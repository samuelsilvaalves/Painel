Nvoid PWM_Set(uint32_t cycle)
N{
N	/*Configura PWM*/
N//	LPC_PWM1->MR0 = cycle;	 
N//	LPC_PWM1->MR1 = cycle * 5/6;  
N//	LPC_PWM1->MR2 = cycle * 5/6;  
N//	LPC_PWM1->MR3 = cycle * 5/6;  
N//	LPC_PWM1->MR4 = cycle * 5/6;  
N//	LPC_PWM1->MR5 = cycle * 5/6;  
N//	LPC_PWM1->MR6 = 0;
N
N
N	LPC_PWM1->MR0 = cycle;	 
X	((LPC_PWM_TypeDef *) ((0x40000000UL) + 0x18000) )->MR0 = cycle;	 
N	LPC_PWM1->MR1 = cycle/2;		/*50% duty cycle*/  
X	((LPC_PWM_TypeDef *) ((0x40000000UL) + 0x18000) )->MR1 = cycle/2;		   
N	LPC_PWM1->MR2 = cycle/2;  
X	((LPC_PWM_TypeDef *) ((0x40000000UL) + 0x18000) )->MR2 = cycle/2;  
N	LPC_PWM1->MR3 = cycle/2; 
X	((LPC_PWM_TypeDef *) ((0x40000000UL) + 0x18000) )->MR3 = cycle/2; 
N	LPC_PWM1->MR4 = cycle/2;  
X	((LPC_PWM_TypeDef *) ((0x40000000UL) + 0x18000) )->MR4 = cycle/2;  
N	LPC_PWM1->MR5 = cycle/2;
X	((LPC_PWM_TypeDef *) ((0x40000000UL) + 0x18000) )->MR5 = cycle/2;
N	LPC_PWM1->MR6 = 0;
X	((LPC_PWM_TypeDef *) ((0x40000000UL) + 0x18000) )->MR6 = 0;
N			  
N	/*The LER will be cleared when the Match 0 takes place, in order to
N	  load and execute the new value of match registers, all the PWMLERs need to
N	  reloaded. all PWM latch enabled*/ 
N	LPC_PWM1->LER = LER0_EN | LER1_EN | LER2_EN; //| LER3_EN | LER4_EN | LER5_EN;
X	((LPC_PWM_TypeDef *) ((0x40000000UL) + 0x18000) )->LER = (1 << 0) | (1 << 1) | (1 << 2); 
N}
