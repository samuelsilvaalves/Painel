Nvoid ext_task(void)
N{
N	static char cnt_ext = 0;
N
N	/*LED para debug IR Receiver*/
N	if(!RECEIVE_IR())
X	if(!((((LPC_GPIO_TypeDef *) ((0x2009C000UL) + 0x00040) )->FIOPIN & (1<<3))))
N	{	
N		LED3_BOARD(1);
X		if(!1)((LPC_GPIO_TypeDef *) ((0x2009C000UL) + 0x00020) )->FIOSET |= (1<<18); else ((LPC_GPIO_TypeDef *) ((0x2009C000UL) + 0x00020) )->FIOCLR |= (1<<18);;
N	}else 
N	   	LED3_BOARD(0);
X	   	if(!0)((LPC_GPIO_TypeDef *) ((0x2009C000UL) + 0x00020) )->FIOSET |= (1<<18); else ((LPC_GPIO_TypeDef *) ((0x2009C000UL) + 0x00020) )->FIOCLR |= (1<<18);;
N
N	if(!atoi(cfg.file.func_ir_scene))	/*Funo IR Scene desabilitada?*/
N	{
N		/*Desabilita interrupo e contadores auxiliares*/
N		NVIC_DisableIRQ(EINT3_IRQn);
N		cnt_ext = 0;
N		contEnableExtIr = 0;
N	 	return;
N	}
N
N	if(!flag_new_cmdir)	/*Ainda sem receber IR?*/
N	{
N		if(contEnableExtIr >= 100)
N			NVIC_EnableIRQ(EINT3_IRQn);
N		cnt_ext = 0;	
N		return;
N	}
N
N	if(++cnt_ext >= 20)	
N	{
N		if(!RECEIVE_IR())
X		if(!((((LPC_GPIO_TypeDef *) ((0x2009C000UL) + 0x00040) )->FIOPIN & (1<<3))))
N		{ 	
N			NVIC_DisableIRQ(EINT3_IRQn);	/*Desabilita a interrupo responsavel por detectar que um IR xegou no pino*/
N			get_new_ir();					/*Chama funo para l o IR e verificar se ele de fato  um comando de cena*/
N			contEnableExtIr = 45;			/*Temporizao para habilitar interrupo novamente*/
N		}else
N			NVIC_EnableIRQ(EINT3_IRQn);		/*Habilita novamente a interrupo*/
N		/*Reseta contador e flag para receber um novo IR como comando de cena*/
N		cnt_ext = 0;
N		flag_new_cmdir = __FALSE;					
X		flag_new_cmdir = 0;					
N	}			
N}	
