N		pulse = j-4;
N
N		for(x=0;x<NUM_SCENE;x++)	/*Loop para comparar o IR recebido com apenas os IRs previamente cadastrados*/
N		{
N			wdt_feed();
X			((LPC_WDT_TypeDef *) ((0x40000000UL) + 0x00000) )->WDFEED = 0xAA; ((LPC_WDT_TypeDef *) ((0x40000000UL) + 0x00000) )->WDFEED = 0x55;;
N			memset(ir,0xFFFF,sizeof(ir));
N			memset(strId,0,sizeof(strId));
N			sprintf(strId,"%s%u.TXT\0",FILE_CMDIR,x);
X			sprintf(strId,"%s%u.TXT\0","CMDIR\\IR_\0",x);
N
N			f = fopen(strId,"r");
N
N			if(f == NULL)
X			if(f == 0)
N			{
N				/*Ocorreu erro na abertura do IR em questo (addr: i)mas continua no loop para abrir o restante dos IRs*/
N				continue;	
N			}
N			
N			id=0;
N			i=0;
N			memset(str,0,sizeof(str));
N			/*Lao para ler todo o IR no SD*/
N			while((ch = fgetc (f)) != EOF)
X			while((ch = fgetc (f)) != (-1))
N			{
N				if(ch == '#')
N				{
N					ir[id] = atoi(str);
N					if(ir[id] <= 16)
N						break;
N					id++;		
N				}else
N				{
N					if(ch != '\r' && ch != '\n')
N					{
N						str[i] = ch;
N						i++;
N					}else
N					{
N						memset(str,0,sizeof(str));
N						i=0;
N					}
N				}
N			}
N			fclose(f);
N
N			/*Loop para eliminar os pulsos de repetio do IR que est gravado no sd*/
N			for(j=4;j<MAX_QTD_PULSOS;j++)
X			for(j=4;j<400;j++)
N			{
N				if(ir[j] == 0xFFFF || ir[j] <= 0x10)
N					break;
N				if(j < 70)
N					continue;
N				for(n=j;n<MAX_QTD_PULSOS;n++)
X				for(n=j;n<400;n++)
N					ir[n] = 0xFFFF;	
N				break;		
N			}
N
N			/*Comparao do IR recebido (ir_cmp) com o IR de endereo 'x' (ir) previamente armazenado no SD*/
N			if(ir[0] != 0xFFFF && ir[1] != 0xFFFF && ir[2] != 0xFFFF)	
N			{	
N				for(j=4,n_acertos=0;j<MAX_QTD_PULSOS;j++)
X				for(j=4,n_acertos=0;j<400;j++)
N				{
N					if(ir[j] != 0xffff && ir[j] >= 0x10)
N					{
N						if(ir_cmp[j] >= (ir[j] - (float)ir[j] * 0.3) && ir_cmp[j] <= (ir[j] + (float)ir[j] * 0.3))	/*Cada pulso est dentro do range de 30%?*/
N							n_acertos++;
N						else
N							break;	
N					}else
N						break;
N				}
N	
N				printf("[IR %u (SD)...Acertos:%u...Pulsos:%u]\r",x,n_acertos,pulse);
N				fflush(stdout);
X				fflush((& __stdout));
N				
N				if(n_acertos >= pulse - 5 && n_acertos <= pulse + 5)
N				{
N					printf("[IR Scene found...Scene %u Run]\r*****\r\r",x);
N					fflush(stdout);
X					fflush((& __stdout));
N					enable_timer(0);
N					
N					exe_scene(x);	/*Executa a cena referente ao endereo do IR recebido (reconhecido)*/
N					debounce_press = DEBOUNCE_PRESS_KEY;
X					debounce_press = 500;
N					debounce_release = DEBOUNCE_RELEASE_KEY;
X					debounce_release = 8000;
N					return 1;
N				}
N			}else
N			{
N				/*No encontrou o IR de endereo i*/
N				printf("[IR %u (SD) not found]\r",x);
N				fflush(stdout);
X				fflush((& __stdout));
N			}
N		}
N		printf("[IR Scene not found]\r*****\r\r");
N		fflush(stdout);	
X		fflush((& __stdout));	
N			
N	}else	/*Falha na recepo de IR*/
N	if(pulse > 5)
N	{
N		printf("[Error CMD IR Scene]\r");
N		fflush(stdout);	
X		fflush((& __stdout));	
N	}
N
N	enable_timer(0);	
N	return 0;
N	
N}
