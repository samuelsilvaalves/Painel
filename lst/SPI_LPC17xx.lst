N/*--------------------------- SendBuf ---------------------------------------*/
N
Nstatic BOOL SendBuf (U8 *buf, U32 sz) {
N  /* Send buffer to SPI interface. */
N  U32 i;
N
N  for (i = 0; i < sz; i++) {
N    LPC_SSP0->DR = buf[i];
X    ((LPC_SSP_TypeDef *) ((0x40080000UL) + 0x08000) )->DR = buf[i];
N    /* Wait if Tx FIFO is full. */
N    while (!(LPC_SSP0->SR & TNF));
X    while (!(((LPC_SSP_TypeDef *) ((0x40080000UL) + 0x08000) )->SR & 0x02));
N    LPC_SSP0->DR;
X    ((LPC_SSP_TypeDef *) ((0x40080000UL) + 0x08000) )->DR;
N  }
N  /* Wait until Tx finished, drain Rx FIFO. */
N  while (LPC_SSP0->SR & (BSY | RNE)) {
X  while (((LPC_SSP_TypeDef *) ((0x40080000UL) + 0x08000) )->SR & (0x10 | 0x04)) {
N    LPC_SSP0->DR;
X    ((LPC_SSP_TypeDef *) ((0x40080000UL) + 0x08000) )->DR;
N  }		   
N  return (__TRUE);
X  return (1);
N}
N
N
N/*--------------------------- RecBuf ----------------------------------------*/
N
Nstatic BOOL RecBuf (U8 *buf, U32 sz) {
N  /* Receive SPI data to buffer. */
N  U32 i;
N
N  for (i = 0; i < sz; i++) {
N    LPC_SSP0->DR = 0xFF; 
X    ((LPC_SSP_TypeDef *) ((0x40080000UL) + 0x08000) )->DR = 0xFF; 
N    /* Wait while Rx FIFO is empty. */
N    while (!(LPC_SSP0->SR & RNE));
X    while (!(((LPC_SSP_TypeDef *) ((0x40080000UL) + 0x08000) )->SR & 0x04));
N    buf[i] = LPC_SSP0->DR;
X    buf[i] = ((LPC_SSP_TypeDef *) ((0x40080000UL) + 0x08000) )->DR;
N  }
N  return (__TRUE);
X  return (1);
N}
