N/*--------------------------- BusSpeed --------------------------------------*/
N
Nstatic BOOL BusSpeed (U32 kbaud) {
N  /* Set an SPI clock to required baud rate. */
N  U32 div;
N
N  div = (__PCLK/1000 + kbaud - 1) / kbaud;
X  div = (120000000/1000 + kbaud - 1) / kbaud;
N  if (div == 0)   div = 0x02;
N  if (div & 1)    div++;
N  if (div > 0xFE) div = 0xFE;
N  LPC_SSP0->CPSR = div;
X  ((LPC_SSP_TypeDef *) ((0x40080000UL) + 0x08000) )->CPSR = div;
N  return (__TRUE);
X  return (1);
N}
