N/*--------------------------- UnInit ----------------------------------------*/
N
Nstatic BOOL UnInit (void) {
N  /* Return SSP interface to default state. */
N
N  LPC_GPIO1->FIODIR   &= ~(1<<21);            /* P1.21 is output             */
X  ((LPC_GPIO_TypeDef *) ((0x2009C000UL) + 0x00020) )->FIODIR   &= ~(1<<21);             
N  LPC_PINCON->PINSEL3 &= ~(0<<10);            /* P1.21 SSEL (used as GPIO)   */
X  ((LPC_PINCON_TypeDef *) ((0x40000000UL) + 0x2C000) )->PINSEL3 &= ~(0<<10);             
N
N  LPC_PINCON->PINSEL3 &= ~(3UL<<8);           /* P1.20 cleared               */
X  ((LPC_PINCON_TypeDef *) ((0x40000000UL) + 0x2C000) )->PINSEL3 &= ~(3UL<<8);            
N  LPC_PINCON->PINSEL3 &= ~((3<<14) | (3<<16));/* P1.23, P1.24 cleared        */
X  ((LPC_PINCON_TypeDef *) ((0x40000000UL) + 0x2C000) )->PINSEL3 &= ~((3<<14) | (3<<16)); 
N
N  LPC_SC->PCLKSEL1 &= ~(3<<10);               /* PCLKSP0 = CCLK/4 ( 25MHz)   */
X  ((LPC_SC_TypeDef *) ((0x40080000UL) + 0x7C000) )->PCLKSEL1 &= ~(3<<10);                
N
N  LPC_SSP0->CPSR      &= ~0xFF; 
X  ((LPC_SSP_TypeDef *) ((0x40080000UL) + 0x08000) )->CPSR      &= ~0xFF; 
N
N  LPC_SSP0->CR0       &= ~0xFFFF;   
X  ((LPC_SSP_TypeDef *) ((0x40080000UL) + 0x08000) )->CR0       &= ~0xFFFF;   
N  LPC_SSP0->CR1       &= ~0x0F;
X  ((LPC_SSP_TypeDef *) ((0x40080000UL) + 0x08000) )->CR1       &= ~0x0F;
N
N  return (__TRUE);
X  return (1);
