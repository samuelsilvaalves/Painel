N/* SSPxSR - bit definitions. */
N#define TFE     0x01
N#define TNF     0x02
N#define RNE     0x04
N#define RFF     0x08
N#define BSY     0x10
N
N/*--------------------------- Init ------------------------------------------*/
N
Nstatic BOOL Init (void) {
N
N  /* Initialize and enable the SSP Interface module. */
N
N  LPC_SC->PCONP       |= (1 << 21);           /* Enable power to SSPI0 block */
X  ((LPC_SC_TypeDef *) ((0x40080000UL) + 0x7C000) )->PCONP       |= (1 << 21);            
N
N  /* SSEL is GPIO, output set to high. */
N  LPC_GPIO1->FIODIR   |=  (1<<21);            /* P1.21 is output             */
X  ((LPC_GPIO_TypeDef *) ((0x2009C000UL) + 0x00020) )->FIODIR   |=  (1<<21);             
N  LPC_GPIO1->FIOPIN   |=  (1<<21);            /* set P1.21 high (SSEL inact.)*/
X  ((LPC_GPIO_TypeDef *) ((0x2009C000UL) + 0x00020) )->FIOPIN   |=  (1<<21);             
N  LPC_PINCON->PINSEL3 &= ~(0<<10);             /* P1.21 SSEL (used as GPIO)   */
X  ((LPC_PINCON_TypeDef *) ((0x40000000UL) + 0x2C000) )->PINSEL3 &= ~(0<<10);              
N 
N//  /* P3.26 is SD Card Power Supply Enable Pin [KIT 1768 BIG]*/
N//  LPC_GPIO3->FIODIR   |=  (1<<26);            /* P3.26 is output             */
N//  LPC_GPIO3->FIOPIN   &= ~(1<<26);            /* set P3.26 low(enable power) */
N
N  /* P1.25 is SD Card inserted [KIT 1768 MINI]*/
N  LPC_GPIO1->FIODIR   &=  ~(1<<25);            /* P1.25 is input          */
X  ((LPC_GPIO_TypeDef *) ((0x2009C000UL) + 0x00020) )->FIODIR   &=  ~(1<<25);             
N  
