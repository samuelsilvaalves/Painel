N/*--------------------------- Send ------------------------------------------*/
N
Nstatic U8 Send (U8 outb) {
N  /* Write and Read a byte on SPI interface. */
N
N  LPC_SSP0->DR = outb;
X  ((LPC_SSP_TypeDef *) ((0x40080000UL) + 0x08000) )->DR = outb;
N  while (LPC_SSP0->SR & BSY);                 /* Wait for transfer to finish */
X  while (((LPC_SSP_TypeDef *) ((0x40080000UL) + 0x08000) )->SR & 0x10);                  
N  return (LPC_SSP0->DR);                      /* Return received value       */
X  return (((LPC_SSP_TypeDef *) ((0x40080000UL) + 0x08000) )->DR);                       
N}
